<script>
  document.addEventListener("DOMContentLoaded", function() {
    let a = 60;
    let timerElement = document.getElementById('timer');
    let verifyOTPButton = document.querySelector('#otpForm button[type="submit"]');
    let resendOTPLink = document.getElementById('ResendOTP');

    let otpTimer = setInterval(() => {
      timerElement.innerText = a--;

      if (a === -1) {
        clearInterval(otpTimer);
        verifyOTPButton.style.display = 'none';
        resendOTPLink.style.display = 'block';
      }
    }, 1000);
  });
</script>

<!-- Login section -->
<section class="ms-login padding-tb-30">
  <div class="container">
    <div class="section-title-2">
      <h2 class="ms-title">OTP VERIFICATION<span></span></h2>
    </div>
    <div class="ms-login-content p-t-15">
      <div class="ms-login-box">
        <div class="ms-login-wrapper">
          <div
            class="ms-login-container"
            style="background-color: antiquewhite"
          >
            <div class="ms-login-form">
              <form action="/views/user/otp" method="post" id="otpForm">
                <span class="ms-login-wrap">
                  <label>Enter OTP</label>
                  <input
                    type="text"
                    name="otp"
                    placeholder="Enter your OTP"
                    required
                  />
                </span>
                <% if(typeof message!=="undefined"){ %>
                  <p style="color: red;">
                    <%= message %>
                </p>
                  <% } %>
                  <button class="ms-btn-2 btn" type="submit">Verify OTP</button>
                </span>
                <div class="form-group">
                  <label>Time Left: </label>
                  <span>
                    <h4 id="timer"></h4>
                  </span>
                  <a href="/resendOTP">
                    <p class="mt-3">Didn't receive OTP?<span id="ResendOTP">Resend</span></p>
                  </a>
                </div>
              </form>                         
            </div>
          </div>
        </div>
      </div>
      <!-- <div class="ms-login-box d-n-991">
        <div class="ms-login-img"></div>
      </div> -->
    </div>
  </div>
</section>
